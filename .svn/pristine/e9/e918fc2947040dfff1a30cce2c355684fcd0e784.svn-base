package com.echo.mobileweb.model;

public class EveryAmountDto {
    private String shopname;
    private String ddname;
    private String jlname;
    private String zjname;
    private Double sysamount;
    private Double amount;
    private Double saleamount;
    private Integer shopcount;

    public String getShopname() {
        return shopname;
    }

    public void setShopname(String shopname) {
        this.shopname = shopname;
    }

    public String getDdname() {
        return ddname;
    }

    public void setDdname(String ddname) {
        this.ddname = ddname;
    }

    public String getJlname() {
        return jlname;
    }

    public void setJlname(String jlname) {
        this.jlname = jlname;
    }

    public String getZjname() {
        return zjname;
    }

    public void setZjname(String zjname) {
        this.zjname = zjname;
    }

    public Double getSysamount() {
        return sysamount;
    }

    public void setSysamount(Double sysamount) {
        this.sysamount = sysamount;
    }

    public Double getAmount() {
        return amount;
    }

    public void setAmount(Double amount) {
        this.amount = amount;
    }

    public Double getSaleamount() {
        return saleamount;
    }

    public void setSaleamount(Double saleamount) {
        this.saleamount = saleamount;
    }

    public Integer getShopcount() {
        return shopcount;
    }

    public void setShopcount(Integer shopcount) {
        this.shopcount = shopcount;
    }
}
